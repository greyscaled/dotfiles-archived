[core]
	editor = vim
[url "git@github.com:"]
	insteadOf = https://github.com/
[mergetool]
	prompt = false
	keepBackup = false
[merge]
	tool = vimdiff
	conflictstyle = diff3
[commit]
	gpgsign = true
[alias]
	clbranches = "! git branch -d $(git branch --merged=master | grep -v master)"
	f = "! git commit --fixup $(git log --pretty='%H' -1 --invert-grep --grep 'fixup! ')"
	fs = "!f() { git commit --fixup :/$1 ; }; f"
